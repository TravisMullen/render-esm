{
  "name": "render-esm",
  "version": "1.0.1",
  "main": "dist/render-esm.cjs.js",
  "module": "dist/render-esm.mjs",
  "dependencies": {
    "camelcase": "^5.0.0",
    "glob": "^7.1.3",
    "semistandard": "*"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "rollup": "^0.66.0",
    "snazzy": "^8.0.0",
    "standard": "*",
    "swap-global": "^1.0.5"
  },
  "scripts": {
    "prepare": "npm run test",
    "prebuild": "npm run lint",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "pretest": "npm run lint && npm run build",
    "test": "mocha --require babel-core/register --reporter spec ./test/entry.js --recursive './test/*.spec.*'",
    "test:purge": "rm ./test/some-test*",
    "lint": "standard --fix './{src,test,bin}/**/*.js' --verbose | snazzy"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "mocha"
    ],
    "globals": [
      "RenderESM",
      "expect",
      "loadModule",
      "statSync",
      "readFileSync",
      "NON_STRINGS",
      "TEST_FILE",
      "TEST_FILE_TYPE",
      "TEST_DATA"
    ]
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "dynamic-import-node"
    ]
  },
  "engines": {
    "node": ">= 8.6"
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "render-esm": "./bin/render-esm"
  },
  "homepage": "https://github.com/TravisMullen/render-esm#readme",
  "bugs": {
    "url": "https://github.com/TravisMullen/render-esm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TravisMullen/render-esm.git"
  },
  "author": "Travis Mullen <travis@travismullen.com> (http://travismullen.com/)",
  "keywords": [
    "esm",
    "module",
    "render-module",
    "render-ecmascript",
    "render-export",
    "ecmascript-optimization",
    "optimization",
    "javascriptmodules",
    "javascript-modules",
    "prerender",
    "pre-render",
    "es6",
    "es2015"
  ]
}
