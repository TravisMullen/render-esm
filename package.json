{
  "name": "base64-to-esm",
  "version": "1.0.0",
  "main": "dist/base64toESM.cjs.js",
  "module": "dist/base64toESM.esm.js",
  "dependencies": {
    "camelcase": "^5.0.0",
    "glob": "^7.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "rollup": "^0.66.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "swap-global": "^1.0.5"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "mocha --require babel-core/register --reporter spec ./test/entry.js --recursive './test/*.spec.*'",
    "test:local": "./bin/base64toESM --target=txt --path=./test/clean-files",
    "lint": "standard --fix './{src,test,bin}/**/*.js' --verbose | snazzy"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "dynamic-import-node"
    ]
  },
  "engines": {
    "node": ">= 8.6"
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "base64toESM": "./bin/base64toESM"
  }
}
